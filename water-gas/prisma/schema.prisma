datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Measure {
  id              Int      @id @default(autoincrement())
  measure_uuid    String   @unique
  confirmed_value Int?
  measure_datetime DateTime
  measure_type    String
  image_url       String
  customer_code   String  // Certifique-se de que este campo est√° definido
  has_confirmed   Boolean  @default(false)
}
